{
  "name": "gulp4-webpack",
  "version": "1.0.0",
  "description": "A modern ES6 Gulp workflow combined with Webpack",
  "scripts": {
    "dev": "gulp --require babel-register --gulpfile tasks",
    "build": "set NODE_ENV=production && gulp build --require babel-register --gulpfile tasks"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "babelrc": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pascalaoms/gulp4-webpack"
  },
  "author": "Pascal Klau",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.23.6",
    "bs-fullscreen-message": "^1.1.0",
    "bs-html-injector": "^3.0.3",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-data": "^1.3.1",
    "gulp-file-include": "^2.0.1",
    "gulp-plumber": "^1.2.0",
    "gulp-pug": "^3.3.0",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.2.1",
    "gulp-sass-glob": "^1.0.8",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.4.0",
    "gulp.spritesmith": "^6.9.0",
    "imports-loader": "^0.8.0",
    "sass-loader": "^6.0.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "gsap": "^2.0.1",
    "jquery": "^3.3.1",
    "scrollmagic": "^2.0.5"
  }
}
